{
  "version": 2,
  "builds": [
    {
      "src": "Assignment-1/moviesAPI/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Assignment-2/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Assignment-1/moviesAPI/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "Assignment-2/$1"
    }
  ]
}
